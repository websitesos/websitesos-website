<!doctype html>
<html lang="en">

  <head>
    <script>performance.mark('HEAD Start');</script>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <link rel="preconnect" href="https://websitesos.co.uk" crossorigin>

    <script>performance.mark('CSS Start');</script>
    <link rel="stylesheet" href="{{ '/assets/css/tasty.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}">
    <script>
      performance.mark('CSS End');
      performance.measure('CSS Time', 'CSS Start', 'CSS End');
    </script>

    <script src="https://www.google-analytics.com/analytics.js" async></script>
    <script src="/assets/js/main.js" async></script>

    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@websitesos">
    <meta name="twitter:domain" content="websitesos.co.uk">
    <meta name="twitter:creator" content="@websitesos">
    <meta name="twitter:title" content="websitesos.co.uk">
    <meta name="twitter:image" content="https://websitesos.co.uk/logo.png">
    <meta name="twitter:description" content="Website Performance Optimisation Specialists">
    <meta property="og:title" content="websitesos.co.uk">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://websitesos.co.uk">
    <meta property="og:image" content="https://websitesos.co.uk/logo.png">

    <link rel="shortcut icon" href="/icon.png">
    <link rel="apple-touch-icon" href="/icon.png">
    <meta name="apple-mobile-web-app-title" content="websitesos">
    <meta name="description" content="{{ description or metadata.description }}">

    <link rel="manifest" href="/site.webmanifest">
    <!-- Place favicon.ico in the root directory -->
    <script>
      performance.mark('HEAD End');
      performance.measure('HEAD Time', 'HEAD Start', 'HEAD End');
    </script>
  </head>

  <body class="page-layout{% if templateClass %} {{ templateClass }}{% endif %}">
    <header>
      <h1 class="site-title"><a href="{{ '/' | url }}">{{ metadata.title }}</a></h1>

      {#- Read more about `eleventy-navigation` at https://www.11ty.dev/docs/plugins/navigation/ #}
      <ul class="nav o-list-bare">
      {%- for entry in collections.all | eleventyNavigation %}
        <li class="nav-item{% if entry.url == page.url %} nav-item-active{% endif %} o-list-bare__item"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
      {%- endfor %}
      </ul>
    </header>

    <script>performance.mark('Content Start')</script>

    <main>
      {{ content | safe }}
    </main>

    {% if showSidebar %}
    <aside>
    </aside>
    {% endif %}

    <script>performance.mark('Content End')</script>

    <footer>
      
    </footer>

    <!-- Current page: {{ page.url | url }} -->

    
  </body>
</html>
